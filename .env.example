# xplat environment variables
# Copy to .env and fill in values

# GitHub sync (sync-gh commands)
# Token with repo, workflow, pages scopes for 5000 req/hour
GITHUB_TOKEN=

# Cloudflare sync (sync-cf commands)
# API token with Account:Read, Audit Logs:Read permissions
CF_API_TOKEN=
CF_ACCOUNT_ID=
CF_WEBHOOK_SECRET=

# Cloudflare worker deployment (sync-cf worker commands)
CLOUDFLARE_API_TOKEN=
CLOUDFLARE_ACCOUNT_ID=
CLOUDFLARE_DOMAIN=
CLOUDFLARE_ZONE_ID=
CLOUDFLARE_PAGE_PROJECT_NAME=
